<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/layout_constraint_paywall_overlay_text_message_received"
    android:layout_width="match_parent"
    android:layout_height="@dimen/paywall_overlay_text_message_received_height"
    android:visibility="gone"
    tools:visibility="visible">


    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/text_view_overlay_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/montserrat_semi_bold"
        android:text="@string/paywall_overlay_label_text_message_received"
        android:textAllCaps="true"
        android:textColor="@color/textMessages"
        android:textSize="@dimen/default_text_size_sub_headline"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>
