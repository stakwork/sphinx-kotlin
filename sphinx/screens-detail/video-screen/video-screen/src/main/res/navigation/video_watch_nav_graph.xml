<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/video_watch_nav_graph"
    app:startDestination="@+id/navigation_video_watch_screen_fragment">

    <fragment
        android:id="@+id/navigation_video_watch_screen_fragment"
        android:name="chat.sphinx.video_screen.ui.watch.VideoFeedWatchScreenFragment"
        tools:layout="@layout/fragment_video_watch_screen">
        <argument
            android:name="argChatId"
            app:argType="long" />
        <argument
            android:name="argFeedId"
            app:argType="string" />
        <argument
            android:name="argFeedUrl"
            app:argType="string" />
        <argument
            android:name="argFeedItemId"
            app:argType="string"
            app:nullable="true"
            android:defaultValue="@null"/>
    </fragment>

</navigation>
