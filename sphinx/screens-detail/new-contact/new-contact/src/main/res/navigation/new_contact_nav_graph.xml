<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/new_contact_nav_graph"
    app:startDestination="@+id/navigation_new_contact_fragment">

    <fragment
        android:id="@+id/navigation_new_contact_fragment"
        android:name="chat.sphinx.new_contact.ui.NewContactFragment"
        tools:layout="@layout/fragment_new_contact">

        <argument
            android:name="argFromAddFriend"
            app:argType="boolean" />

        <argument
            android:name="argPubKey"
            app:argType="string"
            app:nullable="true"
            android:defaultValue="@null" />

        <argument
            android:name="argRouteHint"
            app:argType="string"
            app:nullable="true"
            android:defaultValue="@null" />

    </fragment>

</navigation>
